	    end)
	end)
	spawn(function()
	    pcall(function()
	        local Code = {
	            "SECRET_ADMIN",
	            "CODESLIDE",
	            "ADMINHACKED",
	            "TRIPLEABUSE",
	            "SEATROLLING",
	            "ZIOLES",
	            "KITT_RESET",
	            "Sub2CaptainMaui",
	            "SUB2GAMERROBOT_RESET1",
	            "kittgaming",
	            "Sub2Fer999",
	            "Enyu_is_Pro",
	            "Magicbus",
	            "JCWK",
	            "Starcodeheo",
	            "Bluxxy",
	            "fudd10_v2",
	            "FUDD10",
